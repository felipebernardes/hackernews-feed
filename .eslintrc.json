{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
        "comma-dangle": ["error", "never"],
        "no-new": "off",
        "no-plusplus": "off",
        "prefer-destructuring": "off"
    }
}
